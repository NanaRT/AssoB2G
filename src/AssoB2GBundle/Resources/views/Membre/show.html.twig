{% extends '::base.html.twig' %}

{% block body -%}
	<br><br><br>
	<div class="container">
	    <div class="row">
	        <div class="col-lg-12">
			    <h1>Membre</h1>
			
			    <table class="record_properties">
			        <tbody>
			            <tr>
			                <th>Id</th>
			                <td>{{ entity.id }}</td>
			            </tr>
			            <tr>
			                <th>Nom</th>
			                <td>{{ entity.nom }}</td>
			            </tr>
			            <tr>
			                <th>Prenom</th>
			                <td>{{ entity.prenom }}</td>
			            </tr>
			            <tr>
			                <th>Poste</th>
			                <td>{{ entity.poste }}</td>
			            </tr>
			        </tbody>
			    </table>
			
			        <ul class="record_actions">
			    <li>
			        <a href="{{ path('membre') }}">
			            Back to the list
			        </a>
			    </li>
			    <li>
			        <a href="{{ path('membre_edit', { 'id': entity.id }) }}">
			            Edit
			        </a>
			    </li>
			    <li>{{ form(delete_form) }}</li>
			</ul>
			</div>
		</div>
	</div>
{% endblock %}
